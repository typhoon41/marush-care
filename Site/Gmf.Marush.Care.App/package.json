{
  "name": "Marush",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development,sr",
    "start-en": "ng serve --configuration=development,en",
    "start-ru": "ng serve --configuration=development,ru",
    "build": "ng build && copyfiles -E -u 2 src/assets/**/* dist/browser/assets",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "translate": "ng extract-i18n && sortjson src/locale",
    "serve:ssr": "node dist/server/server.mjs",
    "lint": "eslint --fix && stylelint --fix src/**/*.scss"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.0",
    "@angular/compiler": "19.2.0",
    "@angular/core": "19.2.0",
    "@angular/forms": "19.2.0",
    "@angular/localize": "19.2.0",
    "@angular/platform-browser": "19.2.0",
    "@angular/platform-browser-dynamic": "19.2.0",
    "@angular/platform-server": "19.2.0",
    "@angular/router": "19.2.0",
    "@angular/ssr": "19.2.0",
    "angular-eslint": "19.2.0",
    "air-datepicker": "3.5.3",
    "express": "^4.19.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.0",
    "@angular/cli": "19.2.0",
    "@angular/compiler-cli": "19.2.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/node": "22.13.8",
    "@typhoon41/eslint-config-angular": "1.0.0",
    "@typhoon41/scss-solutions": "0.1.12",
    "@typhoon41/stylelint-config": "1.1.1",
    "copyfiles": "2.4.1",
    "jasmine-core": "~5.4.0",
    "json-sort-cli": "4.0.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
