{
  "name": "Marush",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development,sr",
    "start-en": "ng serve --configuration=development,en",
    "start-ru": "ng serve --configuration=development,ru",
    "build": "ng build && copyfiles -E -u 1 src/proxy-server.mjs dist && copyfiles -E -u 2 src/assets/**/* dist/browser/assets",
    "watch": "ng build --watch --configuration=development",
    "test": "ng test",
    "translate": "ng extract-i18n && sortjson src/locale",
    "serve:ssr": "node dist/proxy-server.mjs",
    "lint": "ng lint --format stylish --fix && stylelint --fix src/**/*.scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.1",
    "@angular/common": "17.3.1",
    "@angular/compiler": "17.3.1",
    "@angular/core": "17.3.1",
    "@angular/forms": "17.3.1",
    "@angular/localize": "17.3.1",
    "@angular/platform-browser": "17.3.1",
    "@angular/platform-browser-dynamic": "17.3.1",
    "@angular/platform-server": "17.3.1",
    "@angular/router": "17.3.1",
    "@angular/ssr": "17.3.1",
    "express": "4.19.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.1",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.1",
    "@angular/compiler-cli": "17.3.1",
    "@types/express": "4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "@typhoon41/eslint-config": "1.0.4",
    "@typhoon41/scss-solutions": "0.1.12",
    "@typhoon41/stylelint-config": "1.0.5",
    "copyfiles": "2.4.1",
    "jasmine-core": "~5.1.0",
    "json-sort-cli": "4.0.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.2.2"
  }
}
